#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/桌面/runma/Front-end/Personal_Project/kindlePC_noteRepair_Tool/node_modules/.store/detect-port-alt@1.1.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/桌面/runma/Front-end/Personal_Project/kindlePC_noteRepair_Tool/node_modules/.store/detect-port-alt@1.1.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../detect-port-alt@1.1.6/node_modules/detect-port-alt/bin/detect-port" "$@"
else
  exec node  "$basedir/../../../../../detect-port-alt@1.1.6/node_modules/detect-port-alt/bin/detect-port" "$@"
fi
