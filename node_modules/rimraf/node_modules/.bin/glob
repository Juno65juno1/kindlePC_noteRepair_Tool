#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/桌面/runma/Front-end/Personal_Project/kindlePC_noteRepair_Tool/node_modules/.store/glob@11.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/桌面/runma/Front-end/Personal_Project/kindlePC_noteRepair_Tool/node_modules/.store/glob@11.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../glob@11.0.0/node_modules/glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../glob@11.0.0/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
